{
    "version": 1,
    "formatters": {
        "standard": {
            "format": "%(asctime)s [%(levelname)s] %(name)s: %(message)s"
        },
        "detailed": {
            "format": "%(asctime)s [%(levelname)s] %(name)s - %(funcName)s:%(lineno)d: %(message)s"
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "level": "DEBUG",
            "formatter": "standard",
            "stream": "ext://sys.stdout"
        }
    },
    "loggers": {
        "setup_django_postgres": {
            "level": "DEBUG",
            "handlers": ["console"],
            "propagate": false
        },
        "file_utils": {
            "level": "DEBUG",
            "handlers": ["console"],
            "propagate": false
        },
        "ytdl_tasks": {
            "level": "DEBUG",
            "handlers": ["console"],
            "propagate": false
        },
        "ytdl_views": {
            "level": "DEBUG",
            "handlers": ["console"],
            "propagate": false
        }
    },
    "root": {
        "level": "INFO",
        "handlers": ["console"]
    }
}
